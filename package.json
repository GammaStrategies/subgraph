    "remove-local": "graph remove --node http://localhost:8020/ gammastrategies/gamma",
    "deploy-local": "graph deploy --node http://localhost:8020/ --ipfs http://localhost:5001 gammastrategies/gamma",
    "prepare:uniswap:bsc": "mustache config/UniswapV3/bsc.json templates/uniV3.template.yaml > subgraph.yaml",
    "prepare:uniswap:mainnet": "mustache config/UniswapV3/mainnet.json templates/uniV3.template.yaml > subgraph.yaml",
    "prepare:uniswap:arbitrum": "mustache config/UniswapV3/arbitrum.json templates/uniV3.template.yaml > subgraph.yaml",
    "prepare:uniswap:polygon": "mustache config/UniswapV3/polygon.json templates/uniV3.template.yaml > subgraph.yaml",
    "prepare:uniswap:optimism": "mustache config/UniswapV3/optimism.json templates/uniV3.template.yaml > subgraph.yaml",
    "prepare:uniswap:celo": "mustache config/UniswapV3/celo.json templates/uniV3.template.yaml > subgraph.yaml",
    "prepare:quickswap:polygon": "mustache config/Quickswap/polygon.json templates/algebra.template.yaml > subgraph.yaml",

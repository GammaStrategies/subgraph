    "deploy:quickswap:polygon": "graph deploy --node https://api.thegraph.com/deploy/  gammastrategies/algebra-polygon",
    "deploy:zyberswap:arbitrum": "graph deploy --node https://api.thegraph.com/deploy/  gammastrategies/zyberswap-arbitrum",
